mixin expandable-checkbox-list(parameters)
    - if (parameters.classList === undefined) {parameters.classList = "";}
    div(class="expandable-checkbox-list " + parameters.classList)
        h3(class="expandable-checkbox-list__title")= parameters.title
        div(class="expandable-checkbox-list__list")
            include ./../checkbox/checkbox.pug
            +checkbox(parameters.checkbox)